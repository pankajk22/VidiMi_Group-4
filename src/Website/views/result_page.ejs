<div class="limiter row">
    <div class="container-login101">
        <div class="w-100" style="text-align: -webkit-center;">
            <div class="wrap-login100">
                <form class="login100-form validate-form" method="POST" action="/admin_login">
                    <span class="login100-form-title"><%= typeof headingStr != 'undefined' ? headingStr : 'Result' %></span>
                    <div class="login100-pic">
                        <img alt="IMG" src="<%= typeof img != 'undefined' ? img : 'error' %>" />
                    </div>
                    <a href="<%= typeof newSearch != 'undefined'? newSearch : '/'%>" type="submit" class="btn btn-primary btn-color" style="color: white; align-content: center; margin-top: 10px;">New Search</a>
                </form>
            </div>
        </div>
    </div>
</div>

<script src="/vendor/jquery/jquery-3.2.1.min.js"></script>
<script src="/vendor/bootstrap/js/popper.js"></script>
<script src="/vendor/bootstrap/js/bootstrap.min.js"></script>
<script src="/vendor/select2/select2.min.js"></script>
<script src="/vendor/tilt/tilt.jquery.min.js"></script>
<script>
    $('.js-tilt').tilt({
        scale: 1.1
    })
</script>